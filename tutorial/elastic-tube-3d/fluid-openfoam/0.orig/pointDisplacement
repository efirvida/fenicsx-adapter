FoamFile
{
    version     2.0;
    format      ascii;
    class       pointVectorField;
    object      pointDisplacement;
}

dimensions      [0 1 0 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    INLET
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }

    INTERFACE
    {
	    type            fixedValue;
        value           $internalField;
    }

    OUTLET
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }

}
